{
    "name": "swanand/tracktik-test",
    "description": "A project to simulate buying of electronic items",
    "authors": [
        {
            "name": "Swanand Reddy Luthmoodoo",
            "email": "swanand.reddy@gmail.com"
        }
    ],
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.3",
        "slim/twig-view": "^3.2",
        "php-di/php-di": "^6.3",
        "vlucas/phpdotenv": "^5.3",
        "middlewares/trailing-slash": "^2.0"
        
    },
    "autoload": {
        "psr-4": {
            "Tracktik\\": "src",
            "Tracktik\\BusinessLogic\\": "src/businessLogic",
            "Tracktik\\BusinessLogic\\Factory\\": "src/businessLogic/factory",
            "Tracktik\\Controller\\": "src/controller",
            "Tracktik\\Controller\\Abstracts\\": "src/controller/abstract",
            "Tracktik\\Controller\\Interfaces\\": "src/controller/interface",
            "Tracktik\\Model\\": "src/model",
            "Tracktik\\Model\\Abstracts\\": "src/model/abstract",
            "Tracktik\\Model\\Interfaces\\": "src/model/interface",
            "Tracktik\\Model\\Entity\\": "src/modle/entity",
            "Tracktik\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },        
        "optimize-autoloader": true        
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
